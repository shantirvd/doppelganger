    <li>
      <p><strong><%= link_to "#{drag.nickname}", drag_path(drag) %></strong></p>
      <p>Accepts events <%= drag.radius %> km around <%= drag.city %></p>
      <p><%= drag.hourly_rate %>â‚¬ per hour</p>
      <p>Specialty: <%= drag.specialty %></p>
